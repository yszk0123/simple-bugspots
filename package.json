{
  "private": true,
  "name": "simple-bugspots",
  "description": "print testcase in tree",
  "version": "0.1.0",
  "main": "lib/index.js",
  "bin": {
    "simple-bugspots": "lib/cli.js"
  },
  "repository": "https://github.com/yszk0123/simple-bugspots.git",
  "author": "Yuji Suzuki <y0suzuki0123@gmail.com>",
  "keywords": [
    "test"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsc -p tsconfig.build.json",
    "build:production": "run-s clean && NODE_ENV=production tsc -p tsconfig.build.json",
    "fix:format": "prettier --write .",
    "fix:lint": "eslint . --fix",
    "fix": "run-s fix:lint fix:format",
    "test:lint": "eslint .",
    "test:jest": "jest",
    "test:typecheck": "tsc --noEmit",
    "test": "run-p test:*"
  },
  "devDependencies": {
    "@types/jest": "29.5.4",
    "jest": "29.6.4",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "p-limit": "^4.0.0",
    "simple-git": "3.19.1"
  }
}
